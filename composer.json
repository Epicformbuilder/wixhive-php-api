{
    "name": "epicformbuilder/wixhive-php-api",
    "description": "WixHive API PHP Library",
    "type": "library",
    "version": "0.0.8",
    "homepage": "https://github.com/epicformbuilder/wixhive-php-api",
    "license": "BSD-3-Clause",
    "keywords": ["php", "wix", "wixhive"],
    "require": {
        "php": ">=8.1.0",
        "guzzlehttp/guzzle": "7.7.0"
    },
    "authors" : [
        {
          "name" : "Epic Form Builder Team",
          "email" : "support@epicformbuilder.com"
        }
    ],
    "require-dev": {
        "phake/phake": "2.0.*@dev",
        "phpunit/phpunit": "4.2.6",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "config": {
        "bin-dir": "./"
    },
    "autoload": {
        "psr-4": { "epicformbuilder\\": "src/epicformbuilder" }
    },
    "minimum-stability": "stable",
    "scripts": {
        "sniffer:php8": "./phpcs -p . --ignore=/vendor/* --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 8.2 -d memory_limit=1024M"
    }

}
